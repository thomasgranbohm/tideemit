generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userId       String   @id
  scheduleLink String?
  courses      Course[]
}

model Course {
  id     Int    @id @default(autoincrement())
  code   String
  name   String
  userId String
  user   User   @relation(fields: [userId], references: [userId])
}
